[alias]
	a = add
	c = commit
	ci = clean -fdx
	co = checkout
	d = diff
	dc = diff --cached
	dt = difftool
	f = fetch
	g = log --graph --full-history --color --decorate --all
	graph = log --graph --full-history --color --decorate --all
	p = pull
	r = rebase
	s = status
[color]
	ui = true
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[merge]
	tool = vimdiff
[pull]
	rebase = true
[user]
	name = David Love
	email = davidklove@pm.me
